@model List<ExchangeViewModel.Exchange_Rates>
@{
    Layout = null;
}



<div class="container">
    <br />
    <table class="table table-bordered">
        <tr>
            <th>Döviz Kodu</th>
            <th>Birim Fiyat</th>
           


        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.currency</td>
                <td>@item.exchange_rate_buy</td>
                <td></td>

            </tr>
        }

    </table>
</div>
